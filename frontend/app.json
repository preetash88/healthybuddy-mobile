      "statusBar": {
        "translucent": true,
        "backgroundColor": "transparent"
      },
      "navigationBar": {
        "backgroundColor": "#00000000"
      },
      "package": "com.anonymous.ruriviaaiapp"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
